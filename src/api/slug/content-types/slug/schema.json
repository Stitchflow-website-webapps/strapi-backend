{
  "kind": "collectionType",
  "collectionName": "slugs",
  "info": {
    "singularName": "slug",
    "pluralName": "slugs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "heroSection": {
      "type": "component",
      "repeatable": false,
      "component": "blogs.blogs-hero-section"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "slugs"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "metaData": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "socialMediaList": {
      "type": "component",
      "repeatable": true,
      "component": "shared.social-media"
    },
    "faqSection": {
      "displayName": "FAQ Section",
      "type": "component",
      "repeatable": true,
      "component": "blogs.faq-section"
    },
    "blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::slug.slug"
    },
    "blogTitle": {
      "type": "string"
    }
  }
}
